<%@users.each do |u| %>
<ul> 
	<li><%=link_to u.name, user_path(u.id)%></li>
</ul>
<% end %>

<%= form_tag('/users', method: 'POST') do %>
	<label>Enter a name to create a new user</label>
	<input type="text" name='user[name]' value='<%=@user.name%>'> </input>
	<input type="submit" value="submit">
<%end%>